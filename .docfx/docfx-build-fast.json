{
    "build": {
        "dest": "_site",
        "dryRun": true,
        "noLangKeyword": true,
        "disableGitFeatures": true,
        "shouldSkipMarkup": true,
        "noRestore": true,
        "allowCompilationErrors": true,
        "EnumSortOrder": "declaringOrder",
        "includePrivateMembers": false,
        "includeExplicitInterfaceImplementations": false,
        "globalMetadata": {
            "_enableSearch": false,
            "pdf": false,

            "_disableNavbar": true,
            "_disableBreadcrumb": true,
            "_disableAffix": true,
            "_disableContribution": true,
            "_noindex": true,

            "_disableToc": false
        },
        "template": [
            "default",
            "modern",
            "template"
        ],
        "content": [
            {
                "files": [
                    "**.yml"
                ],
                "exclude": [
                    "_site/**",
                    "obj/**",
                    "**/.*"
                ]
            }
        ]
    }
}